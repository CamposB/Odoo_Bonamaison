<?xml version="1.0"?>
<data inherit_id="purchase.report_purchaseorder_document">
        <xpath expr="//th[@name='th_description']" position="before">
            <th name="th_image">
                <strong>Imagem</strong>
            </th>
        </xpath>
        <xpath expr="//td[@id='product']" position="before">
            <td name="td_image">
                <span t-field="line.product_id.image_128" t-options="{&quot;widget&quot;: &quot;image&quot;, &quot;style&quot;:&quot;max-height: 64px; display: inline-block&quot;}"/>
            </td>
        </xpath>

    </data>
